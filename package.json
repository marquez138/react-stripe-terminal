{
    "name": "react-stripe-terminal",
    "version": "0.0.2",
    "description": "React bindings for Stripe WebPOS SDK",
    "author": "sedouard",
    "license": "MIT",
    "repository": "https://github.com/sedouard/react-stripe-pos",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "test": "cross-env CI=1 react-scripts test --env=jsdom",
        "test:watch": "react-scripts test --env=jsdom --verbose=false",
        "build": "rollup -c",
        "prettier": "prettier --config ./.prettierrc.yaml --write */**/*.js",
        "start": "rollup -c -w",
        "prepare": "yarn run build",
        "predeploy": "cd example && yarn install && yarn run build",
        "deploy": "gh-pages -d example/build"
    },
    "keywords": [
        "stripe",
        "stripe-terminal",
        "react"
    ],
    "peerDependencies": {
        "prop-types": "^15.5.4",
        "react": "^15.0.0 || ^16.0.0",
        "react-dom": "^15.0.0 || ^16.0.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "!**/node_modules/**",
            "src/modules/**/*.js"
        ]
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.5",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "cross-env": "^5.1.4",
        "cross-fetch": "^3.0.0",
        "eslint": "^5.0.1",
        "eslint-config-standard": "^11.0.0",
        "eslint-config-standard-react": "^6.0.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.8.0",
        "eslint-plugin-react": "^7.10.0",
        "eslint-plugin-standard": "^3.1.0",
        "gh-pages": "^1.2.0",
        "isomorphic-fetch": "^2.2.1",
        "pure-uuid": "^1.5.5",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-scripts": "^1.1.5",
        "request-promise": "^4.2.2",
        "rollup": "^0.62.0",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-commonjs": "^9.1.3",
        "rollup-plugin-node-resolve": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^1.6.2",
        "rollup-plugin-url": "^1.4.0",
        "xmlhttprequest": "^1.8.0"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-runtime": "^6.26.0",
        "crypto": "^1.0.1",
        "crypto-browserify": "^3.12.0",
        "jsdom": "^13.1.0",
        "react-test-renderer": "^16.5.2",
        "uuid": "^3.3.2"
    }
}
